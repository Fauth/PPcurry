<Window x:Class="PPcurry.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PPcurry"
        mc:Ignorable="d"
        Title="PPcurry" Height="350" Width="525">
    <DockPanel Name="MainPanel">
        <StackPanel Name="ComponentsPanel" DockPanel.Dock="Left">
            <Image Name="ComponentResistor" VerticalAlignment="Top" Width="100" Source="pack://siteoforigin:,,,/Resources/resistor.png"/>
        </StackPanel>
        <Grid Name="MainGrid" DockPanel.Dock="Right" />
    </DockPanel>
</Window>
